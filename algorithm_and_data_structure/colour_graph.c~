#include <stdio.h>

void colour_graph(int n, int d) {
    int G[n][n], i, j, a, b;
    for (i = 0 ; i < n; i++) {
	for (j = 0; j < n; j++) {
	    G[i][j] = 0;
	}
    }
    for (i = 0, scanf("%d",&a); i < a; i++) {
    }
}

int main() {
    int n, d;
    scanf("%d%d",&n,&d);
    colour_graph(n,d);
    return 0;
}
